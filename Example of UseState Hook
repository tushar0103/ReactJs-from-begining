

import React, { useState } from 'react'

const UseStateEx = () => {

 const [count, upstate]= useState(0);
    const Incnum=() =>{
         upstate(count+1);
 
    }
    const Decnum= ()=>{
  upstate(count-1);
    }
  return (
    <>
      <h1>{count}</h1>
      <button onClick={Incnum}>Increase</button>
   
      <button onClick={Decnum}>Decrease</button>
    </>
  )
}

export default UseStateEx

